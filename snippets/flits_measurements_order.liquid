<h6 class="add_book">Measurements (in inches)</h6>

<form id="metafields_form">
  <input type="hidden" name="order[id]" value="{{ order.id }}" />
  <div class="metafield_wrapper">
    <div class="metafield_content">
      <p>
        <label>My Height</label>
        <input type="text" id="my_height" name="metafield[accentuate.my_height]"
               value="{{order.metafields.accentuate.my_height}}" placeholder="My Height">
      </p>
      <p>
        <label>Shoulder</label>
        <input type="text" name="metafield[accentuate.shoulder]"
               value="{{order.metafields.accentuate.shoulder}}" placeholder="Shoulder">
      </p>
      <p>
        <label>Upper Bust</label>
        <input type="text" name="metafield[accentuate.upper_bust]"
               value="{{order.metafields.accentuate.upper_bust}}" placeholder="Upper Bust">
      </p>
      <p>
        <label>Bust</label>
        <input type="text" name="metafield[accentuate.bust]" value="{{order.metafields.accentuate.bust}}"
               placeholder="Bust">
      </p>
      <p>
        <label>Under Bust</label>
        <input type="text" name="metafield[accentuate.under_bust]"
               value="{{order.metafields.accentuate.under_bust}}" placeholder="Under Bust">
      </p>
      <p>
        <label>Arm Hole</label>
        <input type="text" name="metafield[accentuate.arm_hole]"
               value="{{order.metafields.accentuate.arm_hole}}" placeholder="Arm Hole">
      </p>
      <p>
        <label>Sleeve Length</label>
        <input type="text" name="metafield[accentuate.sleeve_length]"
